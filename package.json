{
  "name": "Portchain",
  "version": "1.0.0",
  "description": "Code Challenge for Portchain",
  "main": "final.js",
  "scripts": {
    "start": "clear && npx nodemon --watch . --exec ts-node src/final.ts",
    "start-nomon": "tsc --resolveJsonModule && node dist/final.js",
    "build": "tsc --resolveJsonModule",
    "lint-build": "tslint ./src/* && tsc",
    "test": "clear && jest",
    "test:watch": "clear && jest --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.3",
    "ts-lint": "^4.5.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "src/test/.*\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "axios": "^0.21.0"
  }
}
